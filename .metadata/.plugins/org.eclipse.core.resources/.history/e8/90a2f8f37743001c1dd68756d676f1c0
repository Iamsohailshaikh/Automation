//import java.util.*;
public class Arr {
	Arr(int a)
	{
	public  int Arr()
	{
   return a*a;

	}
	
}
	public static void main(String args[])
	{
	
	int[] arr=new int []{45,5,677,3,2,5,78,90};
	for(int i=arr.length-1;i>=0;i--){
	System.out.println(arr[i]);
	}
	Arr Abd=new Arr(4);
			int result = Abd.Arr();
		System.out.println(result);
	}	
	
}

	

